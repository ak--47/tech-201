	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "go",
			"program": "${file}",
			"runtimeArgs": [
				"--inspect"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"env": {
				"NODE_ENV": "dev"
			}
		},
		{
			"type": "bashdb",
			"request": "launch",
			"name": "debug",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/load.sh",
			"args": [],
			"pathBash": "/opt/homebrew/bin/bash",
		},
		{
			"name": "load",
			"type": "node-terminal",
			"request": "launch",
			"command": "bash ${workspaceFolder}/load.sh",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "delete",
			"type": "node-terminal",
			"request": "launch",
			"command": "bash ${workspaceFolder}/load.sh --delete",
			"cwd": "${workspaceFolder}"
		}
	]
}